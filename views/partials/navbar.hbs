<nav class="navbar border border-danger navbar-expand-lg navbar-light py-2 bg-info">
  <a class="navbar-brand" href="#">
        <img src="{{data.brandLogo}}" width="30" height="30" class="d-inline-block align-top" alt="">
  {{data.brandName}}  </a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto ml-auto">
      {{!-- <p> {{data.link}}  </p> --}}
      {{#each data.link}}

      <li class="nav-item active">
        <a class="nav-link" href="{{this.url}}">{{this.links}} <span class="sr-only">(current)</span></a>
      </li>
      {{/each}}
    </ul>
    <form class="form-inline my-2 my-lg-0">
      <input class="form-control mr-sm-2" type="search" placeholder=" {{data.searchName}} " aria-label="Search">
      <button class="btn btn-danger my-2 my-sm-0" type="submit">{{data.placeholder}}  </button>
    </form>
  </div>
</nav>